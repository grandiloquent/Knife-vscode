{
  "name": "Knife",
  "description": "An example for VS Code",
  "version": "0.0.1",
  "publisher": "vscode-samples",
  "repository": "https://github.com/grandiloquent/Knife-vscode",
  "engines": {
    "vscode": "^1.74.0"
  },
  "activationEvents": [
    "onCommand:knife.translateName"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "Command": "knife.translateName",
        "Title": "translateName"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "submenu": "submenus",
          "group": "uncommon"
        }
      ],
      "submenus": [
        {
          "Command": "knife.translateName",
          "When": "editorTextFocus",
          "Group": "uncommon"
        }
      ]
    }
  },
  "scripts": {},
  "devDependencies": {
    "@types/vscode": "^1.73.0"
  }
}